<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ServiceBindActivity">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="crazy p453 点击页面"/>
    <Button
        android:id="@+id/lock_SerBind"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="绑定ServiceBind"/>
    <Button
        android:id="@+id/unlock_SerBind"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="解绑ServiceBind"/>
    <Button
        android:id="@+id/SerBind_status"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="获取ServiceBind状态"/>

</LinearLayout>