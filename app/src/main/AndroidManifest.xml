<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sxs">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--    需要开机接收信息-->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <receiver android:name=".MyBroadcastReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.sxs.MY_BROADCAST"/>
            </intent-filter>
        </receiver>
        <receiver
            android:name=".BootCompleteReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <activity android:name=".ActivityBroadcast" />

        <service android:name=".MyIntentService" />

        <activity android:name=".MySerActivity" />

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".ActivityService" />

        <service android:name=".ServiceFirst" />

        <activity android:name=".activity_qqLogin" />
        <activity android:name=".Activity_Calculator" />
        <activity android:name=".activity_relative_test2" />
        <activity android:name=".activity_relative_test" />
        <activity android:name=".ActivityThread" />
        <activity
            android:name=".ActivityMain"
            android:label="This is SXS-Main"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityNormal" />
        <activity
            android:name=".ActivityDialog"
            android:theme="@android:style/Theme.Dialog" />
    </application>

</manifest>